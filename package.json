{
  "name": "nextjs-demo",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "dev:safe": "rm -rf .next && next dev",
    "build": "next build",
    "start": "next start",
    "lint": "biome check --no-errors-on-unmatched",
    "lint:fix": "biome check --write --no-errors-on-unmatched",
    "format": "biome format --write --no-errors-on-unmatched",
    "type-check": "tsc --noEmit",
    "hooks:install": "lefthook install",
    "hooks:uninstall": "lefthook uninstall"
  },
  "dependencies": {
    "next": "16.0.0",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.7",
    "@tailwindcss/postcss": "^4.1.15",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "lefthook": "^2.0.0",
    "tailwindcss": "^4.1.15",
    "typescript": "^5.9.3"
  }
}
